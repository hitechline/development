{
  "name": "hitechline",
  "version": "1.0.2",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "fix": "make fix",
    "clean": "lerna exec --parallel -- rimraf dist node_modules/.cache",
    "prebuild": "yarn clean",
    "build": "node scripts/build",
    "prepub": "yarn build",
    "pub": "lerna version --conventional-commits && lerna publish from-package --conventional-commits",
    "lint": "make lint",
    "lint:fix": "make lint-fix",
    "prettier": "make prettier",
    "prettier:fix": "make prettier-fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitechline/development.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@types/node": "14.14.22",
    "eslint": "7.18.0",
    "lerna": "3.22.1",
    "make": "0.8.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "typescript": "4.1.3"
  }
}
